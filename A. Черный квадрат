#include <iostream>
#include <vector>
#include <string>

using namespace std;

int main()
{
	vector<int> a(4);
	string str;
	int count = 0;

	for (int i = 0; i < 4; i++)
	{
		cin >> a[i];
	}

	cin.ignore();
	
	// 1 2 3 4
	// 123214

	getline(cin, str);

	for (int i = 0; i < str.length(); i++)
	{
		if (str[i] == '1')
		{
			count += a[0];
		}
		else if (str[i] == '2')
		{
			count += a[1];
		}
		else if (str[i] == '3')
		{
			count += a[2];
		}
		else if (str[i] == '4')
		{
			count += a[3];
		}
	}

	cout << count << endl;

	return 0;
}
