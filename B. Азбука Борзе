#include <iostream>
#include <string>

using namespace std;

int main()
{
	// 0 = .
	// 1 = -.
	// 2 = --

	string str;
	cin >> str;

	for (int i = 0; i < str.size(); i++)
	{
		if (str[i] == '.' )
		{ 
			cout << 0;
		}
		if (str[i] == '-' && str[i + 1] == '.')
		{
			cout << 1;
			i++;
		}
		if (str[i] == '-' && str[i + 1] == '-')
		{
			cout << 2;
			i++;
		}
	}

	return 0;
}
