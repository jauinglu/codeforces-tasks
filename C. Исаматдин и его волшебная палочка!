#include <iostream>
#include <vector>

using namespace std;

void quickSort(vector<int>& arr, int left, int right) 
{
    if (left >= right) return;

    int pivot = arr[(left + right) / 2];
    int i = left, j = right;

    while (i <= j) 
    {
        while (arr[i] < pivot) i++;
        while (arr[j] > pivot) j--;
        if (i <= j) 
        {
            swap(arr[i], arr[j]);
            i++;
            j--;
        }
    }

    quickSort(arr, left, j);
    quickSort(arr, i, right);
}

void solve()
{
    int n;
    cin >> n;

    vector<int> a(n);
    bool has_odd = false, has_even = false;

    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
        if (a[i] % 2 == 0) has_even = true;
        else has_odd = true;
    }

    if (has_odd && has_even)
    {
        quickSort(a, 0, n - 1);
    }

    for (int i = 0; i < n; i++)
    {
        cout << a[i] << " ";
    }

    cout << endl;
}

int main()
{
    int qq;
    cin >> qq;

    while (qq--)
    {
        solve();
    }

    return 0;
}
