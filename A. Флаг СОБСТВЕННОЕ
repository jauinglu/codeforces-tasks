#include <iostream>
#include <vector>

using namespace std;

void solve()
{
    int n, m;
    cin >> n >> m;

    int** a = new int* [n];

    for (int i = 0; i < n; i++)
    {
        a[i] = new int[m];
    }

    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            cin >> a[i][j];
        }
    }

    bool isUnique = true;

    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            if (a[i][0] != a[i][j])
            {
                isUnique = false;
                break;
            }
        }
        if (!isUnique) break;
    }

    if (isUnique) 
    {
        for (int i = 1; i < n; i++)
        {
            if (a[i][0] == a[i - 1][0])
            {
                isUnique = false;
                break;
            }
        }
    }

    cout << (isUnique ? "YES" : "NO") << endl;

    for (int i = 0; i < n; i++)
    {
        delete[] a[i];
    }

    delete[] a;
}

int main()
{
    //int tt;
    //cin >> tt;

    //while (tt--)
    //{
    solve();
    //}

    return 0;
}
