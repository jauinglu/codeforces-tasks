#include <iostream>
#include <vector>
#define ll long long

using namespace std;

int main()
{
	int n;
	cin >> n;
	
	vector<int> a(n);

	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
	}

	int min1 = a[0];

	for (int i = 0; i < n; i++)
	{
		if (a[i] < min1)
		{
			min1 = a[i];
		}
	}

	ll min2 = 10000000000000000;
	bool found = false;

	for (int i = 0; i < n; i++)
	{
		if (a[i] > min1 && a[i] < min2)
		{
			min2 = a[i];
			found = true;
		}
	}

	if (!found)
	{
		min2 = min1;
	}

	cout << min2 << endl;


	return 0;
}
