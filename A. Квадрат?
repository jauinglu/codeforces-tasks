#include <iostream>
#include <vector>

using namespace std;

void solve()
{
	vector<int> a(4);

	for (int i = 0; i < a.size(); i++)
	{
		cin >> a[i];
	}
	
	bool isSquare = true;

	for (int i = 0; i < a.size(); i++)
	{
		if (a[i] != a[0])
		{
			isSquare = false;
			break;
		}
	}

	cout << (isSquare ? "YES" : "NO") << endl;

}

int main()
{
	int tt;
	cin >> tt;

	while(tt--)
	{
		solve();
	}

	return 0;
}
