#include <iostream>
#include <vector>

using namespace std;

void solve()
{
	vector<int> a(4);

	for (int i = 0; i < a.size(); i++)
	{
		cin >> a[i];
	}
	
	int count = 0;

	for (int i = 0; i < a.size(); i++)
	{
		for (int j = 1; j < i; j++)
		{
			if (a[i] == a[j])
			{
				count++;
			}
		}
	}

	if (count == 3) cout << "YES" << endl;
	else cout << "NO" << endl;

}

int main()
{
	int tt;
	cin >> tt;

	while(tt--)
	{
		solve();
	}

	return 0;
}
