#include <iostream>
#include <vector>
#include <string>

using namespace std;

void solve()
{
	int n;
	cin >> n;

	string s, t;

	cin >> s >> t;
	
	string sorted_s = s;
	string sorted_t = t;

	for (int i = 0; i < n; i++)
	{
		for (int j = i + 1; j < n; j++)
		{
			if (sorted_s[i] > sorted_s[j])
			{
				swap(sorted_s[i], sorted_s[j]);
			}

			if (sorted_t[i] > sorted_t[j])
			{
				swap(sorted_t[i], sorted_t[j]);
			}
		}
	}

	cout << ((sorted_s == sorted_t) ? "YES" : "NO") << endl;
	
}

int main()
{
	int qq;
	cin >> qq;

	while(qq--)
	{
		solve();
	}

	return 0;
}

